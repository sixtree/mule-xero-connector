            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="privateKeyLoc" use="required" type="xs:string">
            <xs:annotation>
              <xs:documentation>Configurable</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
